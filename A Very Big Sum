#include <iostream>
#include <vector>
#include <iomanip>
using namespace std;
int main()
{
    long long n=0,sum=0;
    cin>>n;
    vector<long long int> ar(n);
    
    for(int i=0;i<n;i++)
    {
       cin>>ar[i];
    
}
     for(int i=0;i<n;i++)
    {
        sum=sum+ar[i];
    
}

    
   cout<<sum;
    
    
    return 0;
}

#include <iostream>
using namespace std;
int main()
{ 
    int g=0,h=0;
    int a[3];
    int b[3];
    for(int i=0;i<3;i++)
    {
        cin>>a[i];
    }
     for(int i=0;i<3;i++)
    {
        cin>>b[i];
    }
    for(int i=0;i<3;i++)
    {
    if(a[i]>b[i])
    {
        g++;
    }
    if(a[i]<b[i])
    {
        h++;
    }
    
    }   
    cout<<g<<" "<<h;
    
    
    
    
    return 0;
}
